{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/screen(.*)",
      "dest": "/screen/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}