{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/screen/(.*)",
      "dest": "/ui/candidate_portal/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/ui/analytics_dashboard/index.html"
    },
    {
      "src": "/",
      "dest": "/ui/analytics_dashboard/index.html"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 30,
      "memory": 512
    }
  }
}